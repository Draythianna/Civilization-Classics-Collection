            RadioButton[4].Left :=  LabelOption[1].Left;
            RadioButton[4].Width :=  LabelOption[1].Width * 5;
            RadioButton[4].Caption :=  '&Drive';
            RadioButton[4].OnClick :=  @RadioButtonClick;

            RadioButton[5] :=  TNewRadioButton.Create(OptionPage[1]);
            RadioButton[5].Parent :=  OptionPage[1].Surface;
            RadioButton[5].Top :=  LabelOption[1].Top - 80;
            RadioButton[5].Left :=  LabelOption[1].Left;
            RadioButton[5].Width :=  LabelOption[1].Width * 5;
            RadioButton[5].Caption :=  '&Archive';
            RadioButton[5].OnClick :=  @RadioButtonClick;
            cn := False;
          end;

      if c1 = True then
        begin
          OptionPage[0] :=  CreateCustomPage(AfterID, 'Civilization 1 Source?', 'Select Install Choice');
          LabelOption[0] :=  TLabel.Create(OptionPage[0]);
          LabelOption[0].Parent :=  WizardForm;
          LabelOption[0].Top :=  168;
          LabelOption[0].Left :=  6;
          LabelOption[0].Caption :=  '&Civilization 1 Options:';
          RadioButton[0] :=  TNewRadioButton.Create(OptionPage[0]);
          RadioButton[0].Parent :=  OptionPage[0].Surface;
          RadioButton[0].Top :=  LabelOption[0].Top - 160;
          RadioButton[0].Left :=  LabelOption[0].Left;
          RadioButton[0].Width :=  LabelOption[0].Width * 5;
          RadioButton[0].Caption :=  '&ISO';
          RadioButton[0].OnClick :=  @RadioButtonClick;

          RadioButton[1] :=  TNewRadioButton.Create(OptionPage[0]);
          RadioButton[1].Parent :=  OptionPage[0].Surface;
          RadioButton[1].Top :=  LabelOption[0].Top - 120;
          RadioButton[1].Left :=  LabelOption[0].Left;
          RadioButton[1].Width :=  LabelOption[0].Width * 5;
          RadioButton[1].Caption :=  '&Drive';
          RadioButton[1].OnClick :=  @RadioButtonClick;

          RadioButton[2] :=  TNewRadioButton.Create(OptionPage[0]);
          RadioButton[2].Parent :=  OptionPage[0].Surface;
          RadioButton[2].Top :=  LabelOption[0].Top - 80;
          RadioButton[2].Left :=  LabelOption[0].Left;
          RadioButton[2].Width :=  LabelOption[0].Width * 5;
          RadioButton[2].Caption :=  '&Archive';
          RadioButton[2].OnClick :=  @RadioButtonClick
          c1 := False;
        end;

  if SuppressibleMsgBox('Ready to Continue?', mbConfirmation, MB_YESNO, IDYES) = IDNO then
    begin
      ExitProcess(0);
      CancelSetup := 1;
    end
    else
    begin
    CancelSetup := 0;
  PrepareToInstallWithProgressPage.SetProgress(0, 0);
  PrepareToInstallWithProgressPage.Show;

  if SuppressibleMsgBox('Kill All CivGames Running Instances (to Update Files)?', mbConfirmation, MB_YESNO, IDYES) = IDNO then
    begin
      if Result2 = False then
        begin
          MsgBox('Installing:' #13#13 'Suit yourself...Continuing Anyways!', mbConfirmation, MB_OK);
        end
        else
        begin
      // First preinstallation step: suppose to kill currently app you are going to update 
      //PrepareToInstallWithProgressPage.SetText('Exiting CivGames Running Instances'), '');
      ShellExec('taskkill', 'taskkill.exe', '/F /IM Civ.exe', ExpandConstant('{app}'), SW_HIDE, ewWaitUntilTerminated, ResultCode);
      ShellExec('taskkill', 'taskkill.exe', '/F /IM CivNet.exe', ExpandConstant('{app}'), SW_HIDE, ewWaitUntilTerminated, ResultCode);
      ShellExec('taskkill', 'taskkill.exe', '/F /IM Civ2.exe', ExpandConstant('{app}'), SW_HIDE, ewWaitUntilTerminated, ResultCode);

      Try

        //    ExecuteCmd('taskkill', '/F /IM MyApp');
        // Set progress bar to 10%
        PrepareToInstallWithProgressPage.SetProgress(1, 10);

        // Second preinstallation step
        // DoSomething usefull...
        // Set progress bar to 20% after done
        PrepareToInstallWithProgressPage.SetProgress(2, 10);

        // ...do other preinstallation steps till the end
        PrepareToInstallWithProgressPage.SetProgress(10, 10);
      Finally
        PrepareToInstallWithProgressPage.Hide;
  ShellExec('open', 'cmd.exe', '/C copy ' + ExpandConstant('{tmp}') + '\indeo_codecs_disable.reg ' + ExpandConstant('{app}') + '\commmon', '', SW_HIDE, ewWaitUntilTerminated, ResultCode);
end;
end;
end;
end;
