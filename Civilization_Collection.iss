procedure EnsureWorkDir();
begin
  WorkDir := ExpandConstant('{commonappdata}\Temp\CivTemp');

  if not ForceDirectories(WorkDir) then
    Log('ERROR: ForceDirectories failed for WorkDir="' + WorkDir + '"');

  Log('WorkDir=' + WorkDir);
end;

procedure ExtractToWorkDir(const ExtractName: string; const RelPath: string);
var
  Src, Dst, DstDir: string;
begin
  EnsureWorkDir();

  // ExtractTemporaryFile accepts ONLY filename (no folders)
  ExtractTemporaryFile(ExtractName);

  Src := ExpandConstant('{tmp}\' + ExtractName);

  // Use RelPath for destination layout (e.g. x64\lame.exe, Codecs\brotli.dll)
  Dst := WorkDir + '\' + RelPath;

  // Ensure destination subdirectories exist
  DstDir := ExtractFileDir(Dst);
  if (DstDir <> '') and (not DirExists(DstDir)) then
    if not ForceDirectories(DstDir) then
      Log('ERROR: ForceDirectories failed for DstDir="' + DstDir + '"');

  if not FileCopy(Src, Dst, False) then
    Log('ERROR: Failed to copy "' + Src + '" to "' + Dst + '"')
  else
    Log('Copied "' + Src + '" -> "' + Dst + '"');
end;

procedure ExtractToWorkDirFlat(const ExtractName: string);
begin
  // Flat = filename only, no subdirectories
  ExtractToWorkDir(ExtractName, ExtractName);
end;

