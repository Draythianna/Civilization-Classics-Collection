procedure DeinitializeSetup();
var
  ResultCode: Integer;
  CivTempDir: string;
  TmpDir: string;
begin
  CivTempDir := ExpandConstant('{commonappdata}\Temp\CivTemp');
  TmpDir := ExpandConstant('{tmp}');

  // If you want different behavior on cancel vs success:
  if CancelSetup = 1 then
  begin
    ShellExec('open', 'cmd.exe',
      '/C rd /s /q "' + CivTempDir + '"',
      '', SW_HIDE, ewWaitUntilTerminated, ResultCode);

    ShellExec('open', 'cmd.exe',
      '/C rd /s /q "' + TmpDir + '"',
      '', SW_HIDE, ewWaitUntilTerminated, ResultCode);
  ExitProcess(0)
  end
  else
  begin
    ShellExec('open', 'cmd.exe',
      '/C rd /s /q "' + TmpDir + '"',
      '', SW_HIDE, ewWaitUntilTerminated, ResultCode);

    ShellExec('open', 'cmd.exe',
      '/C rd /s /q "' + CivTempDir + '"',
      '', SW_HIDE, ewWaitUntilTerminated, ResultCode);
  end;

  BASS_Free;
end;

