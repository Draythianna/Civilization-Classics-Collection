const
  PM_REMOVE = 1;

type
  TMyPoint = record
    X, Y: LongInt;
  end;

  TMyMsg = record
    hwnd: LongWord;
    message: LongWord;
    wParam: LongWord;
    lParam: LongInt;
    time: LongWord;
    pt: TMyPoint;
  end;

function PeekMessageW(var lpMsg: TMyMsg; hWnd: LongWord;
  wMsgFilterMin, wMsgFilterMax, wRemoveMsg: LongWord): Boolean;
  external 'PeekMessageW@user32.dll stdcall';

function TranslateMessage(const lpMsg: TMyMsg): Boolean;
  external 'TranslateMessage@user32.dll stdcall';

function DispatchMessageW(const lpMsg: TMyMsg): LongWord;
  external 'DispatchMessageW@user32.dll stdcall';

function GetTickCount: LongWord;
  external 'GetTickCount@kernel32.dll stdcall';

procedure PumpMessages;
var
  Msg: TMyMsg;
begin
  while PeekMessageW(Msg, 0, 0, 0, PM_REMOVE) do
  begin
    TranslateMessage(Msg);
    DispatchMessageW(Msg);
  end;
end;

procedure ShowSplash(const FileName: string; DurationMS: LongWord);
var
  Start: LongWord;
  SplashForm: TSetupForm;
  SplashImage: TBitmapImage;
begin
  SplashForm := CreateCustomForm;
  SplashForm.Position := poScreenCenter;
  SplashForm.BorderStyle := bsNone;

  SplashImage := TBitmapImage.Create(SplashForm);
  SplashImage.Parent := SplashForm;
  SplashImage.AutoSize := True;
  SplashImage.Bitmap.LoadFromFile(FileName);

  SplashForm.ClientWidth := SplashImage.Width;
  SplashForm.ClientHeight := SplashImage.Height;

  SplashForm.Show;

  Start := GetTickCount;
  while (GetTickCount - Start) < DurationMS do
  begin
    PumpMessages;
    Sleep(10);
  end;

  SplashForm.Close;
  SplashForm.Free;
end;

