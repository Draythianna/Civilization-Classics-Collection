procedure CurStepChanged(CurStep: TSetupStep);
var 
  ResultCode2: Integer;
  ResultCode1: Integer;
begin
if CurStep = ssPostInstall then
begin
  if IsComponentSelected('Common') then
  begin
  MsgBox('InitializeSetup:' #13#13 'Beginning Extraction This Could Take A While!!', mbConfirmation, MB_OK);
  ShellExec('open', 'cmd.exe', '/C echo a | xcopy /S /C /H /R /Y ' + ExpandConstant('{tmp}') + '\*.* %commonappdata%\Temp\CivTemp\','', SW_SHOW, ewWaitUntilTerminated, ResultCode);
  if ShellExec('open', 'cmd.exe', '/C ' + '%commonappdata%\Temp\CivTemp\extract.bat','', SW_SHOW, ewWaitUntilTerminated, ResultCode1) then
    begin
      Log('Succeeded running batch file');
    end
